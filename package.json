{
  "name": "sourc-editor",
  "version": "0.0.6",
  "description": "Lightweight modern online code editor",
  "main": "js/index.js",
  "bugs": {
    "url": "https://github.com/Th3OneAndOnly/Sourc/issues",
    "email": "209th3oneandonly@gmail.com"
  },
  "scripts": {
    "start": "npx concurrently --names \"npm run build:watch\" \"npm run dev:build:watch\" \"npx live-server . --open=demo/demo-app\"",
    "doc": "npx typedoc",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "test": "npx jest",
    "test:cov": "npx jest --coverage",
    "dev:ts": "node dev-ts.js",
    "dev:js": "node dev-js.js",
    "dev:build": "node demo/build.js N",
    "dev:build:watch": "node demo/build.js Y"
  },
  "keywords": [
    "editor",
    "code",
    "web",
    "sourc"
  ],
  "repository": {
    "directory": ".",
    "url": "https://github.com/Th3OneAndOnly/Sourc"
  },
  "author": "TheOneAndOnly",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "concurrently": "^7.0.0",
    "jest": "^27.1.0",
    "live-server": "^1.2.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "tslib": "^1.14.1"
  }
}
